<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Administrator Dashboard</title>

    <!-- Bootstrap core CSS -->
    <link href="css/bootstrap.min.css" rel="stylesheet">

    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.13/angular.js"></script>
    <script src="/admin/js/app.js"></script>

  </head>

  <body>
    <div class="container">
      <h1><%= survey.surveyid%></h1>
      <hr>
      <!-- Loop this -->
      <section class = "box">
        <% for (var i = 0; i < survey.survey.length; i++) { %>
        <% var section = survey.survey[i]; %>

        <!-- content -->
        <div id="<%= section.service.name %>">
          <h2><%= section.service.name %></h2>
          <% for (var v = 0; v < section.questions.length; v++) { %>
          <% var questionItem = section.questions[v] %>
            <h3><%= v+1 %>. <%= questionItem.question.text %></h3>

            <button type="button" class="btn btn-success">
            <%= questionItem.answer%>
            </button>
          <% } %>
          <br><br>
        </div>
        <% } %>
      </section>
    </div>

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
  </body>
</html>
