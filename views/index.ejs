					<!-- Logo -->
						<h1><a href="index.html">One Stop Shop </a></h1>

					<!-- Nav -->
						<nav id="nav">
							<ul>
								<li class="current"><a href="index.html">Home</a></li>
								<li>
									<a href="">Agencies by Name</a>
									<ul>
										<li><a href="https://secure.in.gov/bmv/">Bureau of Motor Vehicles (BMV)</a></li>
										<li><a href="https://secure.in.gov/idoi/">Department of Insurance (DOI)</a></li>
										<li><a href="https://secure.in.gov/dwd/">Department of Workforce Development (DWD)</a></li>
										<li><a href="https://secure.in.gov/fssa/">Family and Social Services (FSSA)</a></li>
										<li><a href="https://secure.in.gov/ihcda/">Indiana Housing and Community Development Authority (IHCDA)</a></li>
										<li><a href="https://secure.in.gov/isdh/">Indiana State Department of Health (ISDH)</a></li>
										<li><a href="https://secure.in.gov/ofbci/">Serve Indiana (SI)</a></li>
									</ul>
								</li>
								<li><a href="agencies_services.html">Agencies by Services</a></li>
								<li><a href="survey.html">Survey</a></li>
							</ul>
						</nav>

					<!-- Banner -->
						<section id="banner">
							<header>
								<h2>Welcome, this is the One Stop Shop.</h2>
								<p>There are number of State services available to you here.
								If you know which one you are familiar with, feel free to click
								on the image or link provided. Otherwise, please answer the questions below to
								determine your family needs.</p>
							</header>
						</section>

					<!-- Intro -->
						<section id="intro" class="container">
							<div class="row">
								<div class="4u">
									<section class="first">
										<a href="https://secure.in.gov/dhs/" class="image responsive"><img src="images/IDHS.png" alt="" /></a>
										<header>
											<h2>Indiana Department of Homeland Security</h2>
										</header>
										<!--
										<p>IDHS will provide statewide leadership, exemplary sutomer service,
										and subject matter expertise for the enhacement of public and private
										partnerships and the assurance of local, state and federal collaboration
										to continually develop Indiana's public safety capabilities for the
										wellbeing and protection of our citizens, property and economy.</p>-->
									</section>
								</div>
								<div class="4u">
									<section class="middle">
										<a href="www.in.gov" class="image responsive"><img src="images/IndyGov.png" alt="" /></a>
										<header>
											<h2>Indiana Goverment</h2>
										</header>
										<!--
										<p>Nisl amet dolor sit ipsum veroeros sed blandit consequat veroeros et magna tempus.</p>
										-->
									</section>
								</div>
								<div class="4u">
									<section class="last">
										<a href="https://secure.in.gov/bmv/" class="image responsive"><img src="images/BMV.png" alt="" /></a>
										<header>
											<h2>Bureau of Motor Vehicles</h2>
										</header>
										<!--
										<p>All BMV transactions can be completed,
											including replacement driverâ€™s license, identification card, titles, registrations or license plates.</p>
										-->
									</section>
								</div>
							</div>
							<footer>
								<ul class="actions">
									<li><a href="survey.html" class="button big">Survey</a></li>
									<!-- <li><a href="#" class="button alt big">Learn More</a></li> -->
								</ul>
							</footer>
						</section>

				</div>
			</div>

		<!-- Main -->
			<div id="main-wrapper">
				<div class="container">
					<div class="row">
						<div class="12u">

							<% if (agencies.length) { %>
							<!-- Portfolio -->
								<section>
									<header class="major">
										<h2>All Departments</h2>
									</header>

									<% for (var i = 0; i < agencies.length; i+=3) { %>
									<div class="row">

										<% for (var j = 0; j < 3 && i+j < agencies.length; j++) { %>
										<% var agency = agencies[i+j]; %>
										<div class="4u">
											<section class="box">

												<center>
													<a href="#" class="image responsive">
														<% if ( agency.imageUrl ) { %>
														<img src="<%=agency.imageUrl%>" alt="" />
														<% } else { %>
														<img src="images/flag.svg" alt="" />
														<% } %>
													</a>
												</center>
												<header>
													<center><h2><%= agency.name %></h2></center>
												</header>
												<div id = ".short-list">
													<%= agency.description %>
												</div>
												<div id = "services">

													<% if ( agency.services.length > 0 ) { %>
													<ul>
														<% agency.services.forEach( function(service) { %>
															<li><%= service.name %></li>
														<% }); %>
													</ul>
													<% }; %>
												</div>
												<a id = "btn">
													more details
												</a>
												<footer>
													<a href="<%=agency.url%>" class="button alt">Visit our website</a>
												</footer>
											</section>
										</div>
										<% } %>

									</div>
									<% } %>
								<!--
								<div id = "agencyCol">
									<script src="../models/agency.js">
									</script>
								</div>
								-->
								</section>
								<% } %>

						</div>
					</div>
				</div>
			</div>

		<!-- Footer -->
			<div id="footer-wrapper">
				<section id="footer" class="container">
					<div class="row">
						<div class="8u">
							<section>
								<header>
									<h2>Blandit nisl adipiscing</h2>
								</header>
								<ul class="dates">
									<li>
										<span class="date">Jan <strong>27</strong></span>
										<h3><a href="#">Lorem dolor sit amet veroeros</a></h3>
										<p>Ipsum dolor sit amet veroeros consequat blandit ipsum phasellus lorem consequat etiam.</p>
									</li>
									<li>
										<span class="date">Jan <strong>23</strong></span>
										<h3><a href="#">Ipsum sed blandit nisl consequat</a></h3>
										<p>Blandit phasellus lorem ipsum dolor tempor sapien tortor hendrerit adipiscing feugiat lorem.</p>
									</li>
									<li>
										<span class="date">Jan <strong>15</strong></span>
										<h3><a href="#">Magna tempus lorem feugiat</a></h3>
										<p>Dolore consequat sed phasellus lorem sed etiam nullam dolor etiam sed amet sit consequat.</p>
									</li>
									<li>
										<span class="date">Jan <strong>12</strong></span>
										<h3><a href="#">Dolore tempus ipsum feugiat nulla</a></h3>
										<p>Feugiat lorem dolor sed nullam tempus lorem ipsum dolor sit amet nullam consequat.</p>
